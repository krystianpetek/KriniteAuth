@using KriniteAuthServer.ResourceDataAppHybrid.Models.ViewModels;
@model UserInfoViewModel
@{
    ViewData["Title"] = "Admin Only";
}

<h1>Admin Only</h1>
<table class="table">
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.UserInfo)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
            </tr>
        }
    </tbody>
</table>
